{
	"_version": "1.12.0",
	"sap.app": {
		"id": "streuobst.webapp",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "streuobst.webapp.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.uxap": {}
			}
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "streuobst.webapp.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
				},
				"async": true,
				"homeRoute": "home"
			},
			"routes": [
				{
					"pattern": "",
					"name": "map",
					"target": [
						"map"
					]
				},{
					"pattern": "detail",
					"name": "treeDetail",
					"target": [
						"treeDetail"
					]
				}
			],
			"targets": {
				"map": {
					"viewName": "Map",
					"controlAggregation": "beginColumnPages"
				},
				"treeDetail": {
					"viewName": "TreeDetail",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}